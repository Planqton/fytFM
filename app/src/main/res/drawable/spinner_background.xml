            android:fromDegrees="0"
            android:toDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%">
            <vector
                android:width="24dp"
                android:height="24dp"
                android:viewportWidth="24"
                android:viewportHeight="24">
                <path
                    android:fillColor="#666666"
                    android:pathData="M7,10l5,5 5,-5z"/>
            </vector>
        </rotate>
    </item>
</layer-list>
